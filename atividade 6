#include <stdio.h>

int main() {
	int i;
    int vetor[10];

    for ( i = 0; i < 10; i++) {
        if (i % 2 == 0) {
            vetor[i] = 7;
        } else {
            vetor[i] = -2;
        }
    }

    printf("Vetor: ");
    for ( i = 0; i < 10; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    return 0;
}
